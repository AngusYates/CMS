---
layout: oclareBase.njk
---
{% include "partials/header.njk" %}
<div class="padding-md oclare-background-default">
    <div class="col-md-3 oclare-background-color-5 border border-dark margin-top-doc">
        <ul>
            <li>
                <a href="#" class="docLink">Introduction</a>
            </li>
        {%- for doc in collections.doc -%}
                <li>
                    <a href="{{ doc.url }}" class="docLink">{{ doc.data.title }}</a>
                </li>
        {%- endfor -%}
        </ul>
    </div>
    <div class="col-md-9">
        <div class="padding-sm oclare-background-color-5" id="article-body">
            <div class="oclare-background-color-4 padding-x-sm" id="article-title">
                <h1 class="margin-0 padding-sm">{{ title }}</h1>
            </div>
            {{content | safe}}
        </div>
    </div>
</div>